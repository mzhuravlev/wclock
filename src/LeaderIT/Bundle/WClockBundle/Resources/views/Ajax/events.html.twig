<div>
    <p><b>Список событий за {{ date }}</b></p>
    <p>время работы: {{ time }}</p>
    <ul>
    {% for event in result %}
        <li class="event-field" data-time="{{ event.time|date("H:i") }}" data-id="{{ event.id }}"><b>{{ event.time|date("H:i") }}</b> - {{ event.type }}</li>
    {% endfor %}
    </ul>
</div>